<?xml version="1.0"?>
<!--
A sphere large enough for a player character. Useful to visualize a barrier.
481 particles.
-->
<effect>
	<particle
		position-x = "0"
		position-y = "0"
		position-z = "8"
		>
		<emitter>
		        <property name="image" value="graphics/particles/shadow-small-circle.png" />
			<property name="follow-parent" />
			<property name="lifetime" value="1"/>

		</emitter>
	</particle>
</effect>
